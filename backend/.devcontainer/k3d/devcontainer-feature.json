{
    "id": "k3d",
    "version": "1.0.0",
    "name": "k3s based kubernetes cluster in docker",
    "postCreateCommand": "k3d --version",
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/bash-command"
    ],
    "options": {
    "k3d_version": {
      "type": "string",
      "description": "k3d version to install",
      "proposals": ["latest", "5.7.4"],
      "default": "latest"
    }
  }
}
